      "data": {
        "date": "2025-09-16",
        "title": "Psychology's WEIRD Problem",
        "type": "Research",
        "action": "Exploring",
        "mainArea": "Speculation",
        "areas": [
          "Data ethics",
          "Critical design",
          "Algorithm studies",
          "Sociology of technology",
          "Critical data studies",
          "Digital governance"
        ],
        "tags": [
          "Cognitive bias"
        ],
        "desc": "Cross-Cultural Perception Studies: Early psychological research relied heavily on WEIRD populations, wrongly generalizing results to all humans and pathologizing non-WEIRD practices. Today, scholars stress the need for culturally sensitive methods to better reflect human diversity.\n",
        "links": [
          "https://www.psychologytoday.com/us/blog/non-weird-science/202004/psychologys-weird-problem https://www.cambridge.org/core/elements/psychologys-weird-problems/C324108A678435B4F18EF712EFB793BB https://thedecisionlab.com/insights/society/behavioral-science-is-weird-and-this-should-concern-us https://www.youtube.com/watch?v=vjB6KG-0bFk"
        ],
        "files": []
      }
